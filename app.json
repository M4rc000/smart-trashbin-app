{
  "expo": {
    "name": "Tong-setan",
    "slug": "tong-setan",
    "version": "1.0.0",
    "scheme": [
      "tong-setan"
    ],
    "orientation": "portrait",
    "icon": "./assets/images/splash.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/splashs.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "tong-setan"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/splash.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "INTERNET"
      ],
      "package": "com.prime_10.tongsetan"
    },
    "web": {
      "favicon": "./assets/images/splash.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-notifications",
        {
          "icon": "./assets/images/splash.png",
          "color": "#ffffff",
          "sounds": [
            "./assets/sound/beebom.wav"
          ]
        }
      ]
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "34648d0e-1a65-4eb0-b745-075113e9565c"
      }
    },
    "owner": "prime_10"
  }
}
